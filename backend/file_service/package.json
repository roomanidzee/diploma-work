    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "mime-types": "^2.1.26",
    "mongoose": "^4.13.20",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.0",
    "routing-controllers-openapi": "^1.8.0",
    "swagger-ui-express": "^4.1.3",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.7",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.2",
    "@types/express-fileupload": "^1.1.0",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.39",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
