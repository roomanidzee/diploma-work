{
  "name": "file_service",
  "version": "0.0.1",
  "description": "File serving service for backend",
  "main": "src/app.ts",
  "author": "Andrey Romanov",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start-dev": "nodemon",
    "start-prod": "pm2 start pm2.json"
  },
  "nodemonConfig": {
    "watch": [
      "src"
