      "src/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/app.ts"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "config-yml": "^0.10.3",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.0",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/redis": "^2.8.14",
    "express-status-monitor": "^1.2.8",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "pm2": "^4.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "typescript-logging": "^0.6.4"
  }
