    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "express-status-monitor": "^1.2.8",
    "mocha": "^7.0.1",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "source-map-support": "^0.5.16",
    "ts-log-debug": "^5.1.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
